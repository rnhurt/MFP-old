<table class="dataTable">
  <thead>
    <tr>
      <th width="100">SSN</th>
      <th>First Name</th>
      <th>Middle Name</th>
      <th>Last Name</th>
      <th>DOB</th>
    </tr>
  </thead>
  <tbody>
    <% @people.each do |person| %>
      <tr>
        <%= content_tag :td, (link_to person.ssn, {:action => 'edit', :id => person}) %>
        <%= content_tag :td, person.first_name %>
        <%= content_tag :td, person.middle_name %>
        <%= content_tag :td, person.last_name %>
        <% if person.dob %>
          <%= content_tag :td, person.dob.strftime("%m/%d/%Y") %>
        <% else %>
          <td></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>